{
  "name": "osnova-react-environment",
  "version": "0.26.0",
  "description": "Modern ready-to-use environment for quick start of a new react application development.",
  "keywords": [
    "webpack",
    "babel",
    "es6",
    "react",
    "redux",
    "boilerplate",
    "osnova"
  ],
  "author": "Aleksandr Snov <snov.contact@gmail.com>",
  "license": "MIT",
  "main": "server/main.js",
  "readme": "README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/Noviel/osnova-react-environment.git"
  },
  "scripts": {
    "build": "npm run build-server && npm run build-client",
    "build:production": "npm run build-server:production && npm run build-client:production",
    "build-server": "better-npm-run build-server",
    "build-client": "better-npm-run build-client",
    "build-server:production": "better-npm-run build-server:production",
    "build-client:production": "better-npm-run build-client:production",
    "watch": "concurrently -k \"npm run watch-client\" \"npm run watch-server\"",
    "watch-client": "better-npm-run watch-client",
    "watch-server": "better-npm-run watch-server",
    "nodemon": "nodemon server/main.js --config dev/nodemon.json",
    "major-release": "npm --no-git-tag-version version major && npm publish",
    "minor-release": "npm --no-git-tag-version version minor && npm publish",
    "patch-release": "npm --no-git-tag-version version patch && npm publish",
    "heroku:get:mongodb-uri": "heroku config:get MONGODB_URI",
    "start": "node server/main.js",
    "prepublish": "npm run build"
  },
  "betterScripts": {
    "build-server": {
      "command": "webpack --config dev/webpack.config.server.js"
    },
    "build-client": {
      "command": "webpack --config dev/webpack.config.client.js"
    },
    "watch-server": {
      "command": "better-npm-run build-server --watch --colors"
    },
    "watch-client": {
      "command": "better-npm-run build-client --watch --colors"
    },
    "build-server:production": {
      "command": "better-npm-run build-server",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build-client:production": {
      "command": "better-npm-run build-client",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "dependencies": {
    "better-npm-run": "^0.0.14",
    "body-parser": "^1.16.1",
    "cookie-parser": "^1.4.3",
    "deepmerge": "^1.3.2",
    "osnova": "^0.31.5",
    "osnova-cluster-launcher": "^1.11.15",
    "osnova-module-socket.io": "^0.4.5",
    "passport": "^0.3.2",
    "passport.socketio": "^3.7.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-helmet": "^4.0.0",
    "react-redux": "^5.0.2",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.2",
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-class-properties": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.22.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-register": "^6.22.0",
    "chokidar": "^1.6.1",
    "clean-webpack-plugin": "^0.1.15",
    "concurrently": "^3.1.0",
    "css-loader": "^0.26.1",
    "eslint": "^3.15.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.9.0",
    "extract-text-webpack-plugin": "2.2.0",
    "file-loader": "^0.10.0",
    "jest": "^18.1.0",
    "postcss-autoreset": "^1.2.1",
    "postcss-import": "^9.1.0",
    "postcss-initial": "^1.5.3",
    "postcss-loader": "^1.2.2",
    "postcss-use": "^2.3.0",
    "precss": "^1.4.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "2.2.0",
    "webpack-assets-manifest": "^0.6.1",
    "webpack-chunk-hash": "^0.4.0",
    "webpack-node-externals": "^1.5.4"
  }
}
